[{
  "selector": "core",
  "style": {
    "selection-box-color": "#AAD8FF",
    "selection-box-border-color": "#8BB0D0",
    "selection-box-opacity": "0.5"
  }
}, {
  "selector": "node",
  "style": {
    "width": "20",
    "height": "20",
    "font-size": "12px",
    "text-valign": "center",
    "text-halign": "center",
    "background-color": "#555",
    "text-outline-color": "#555",
    "text-outline-width": "1px",
    "color": "#fff",
    "overlay-padding": "6px",
    "z-index": "100"
  }
}, {
  "selector": "node[?attr]",
  "style": {
    "shape": "rectangle",
    "background-color": "#aaa",
    "text-outline-color": "#aaa",
    "width": "16px",
    "height": "16px",
    "font-size": "6px",
    "z-index": "1"
  }
}, {
  "selector": "node[?query]",
  "style": {
    "background-clip": "none",
    "background-fit": "contain"
  }
}, {
  "selector": "node:selected",
  "style": {
    "content": "data(id)",
    "border-width": "6px",
    "border-color": "#AAD8FF",
    "border-opacity": "0.5",
    "background-color": "#77828C",
    "text-outline-color": "#77828C"
  }
}, {
  "selector": "edge",
  "style": {
    "color": "black",
    "edge-text-rotation": "autorotate",
    "curve-style": "unbundled-bezier",
    "overlay-padding": "1px",
    "text-margin-y": "1em",
    "target-arrow-shape": "triangle-backcurve",
    "target-arrow-color": "mapData(similarity, 0, 1, black, deepskyblue)",
    "opacity": "0.8",
    "line-color": "mapData(similarity, 0, 1, black, deepskyblue)",
    "width": "mapData(similarity, 0, 1, 0.5, 5)"
  }
}, {
  "selector": "edge:active",
  "style": {
    "content": "data(show_sim)"
  }
}, {
  "selector": "node.unhighlighted",
  "style": {
    "opacity": "0.2"
  }
}, {
  "selector": "edge.unhighlighted",
  "style": {
    "opacity": "0.05"
  }
}, {
  "selector": ".highlighted",
  "style": {
    "z-index": "999999"
  }
}, {
  "selector": "node.highlighted",
  "style": {
    "border-width": "6px",
    "border-color": "#AAD8FF",
    "border-opacity": "0.5",
    "background-color": "#394855",
    "text-outline-color": "#394855"
  }
}, {
  "selector": "edge.filtered",
  "style": {
    "opacity": "0"
  }
}]
